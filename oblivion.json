{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "author": "Goran Legris",
  "name": "Oblivion",
  "themes": [
    {
      "name": "Oblivion",
      "appearance": "dark",
      "style": {
        "background": "#282C34",
        "panel.background": "#22252B",
        "surface.background": "#4E78CC80",
        "elevated_surface.background": "#282C34",
        "editor.background": "#282C34",
        "status_bar.background": "#22252B",
        "editor.subheader.background": "#22252B",
        "title_bar.background": "#22252B",
        "toolbar.background": "#282C34",

        "tab_bar.background": "#22252B",
        "tab.active_background": "#282C34",
        "tab.inactive_background": "#22252B",

        "editor.highlighted_line.background": "#2C313C",
        "editor.active_line_number": "#A4A3A3",
        "editor.line_number": "#606366",
        "editor.gutter.background": "#282C34",
        "editor.active_line.background": "#2C313C",

        "scrollbar.track.background": "#282C3400",
        "scrollbar_thumb.background": "#55575980",
        "scrollbar.thumb.hover_background": "#55575980",
        "scrollbar.thumb.border": "#21252C00",
        "scrollbar.track.border": "#21252C00",

        "element.background": "#22252B00",
        "element.hover": "#323844",
        "element.active": "#4E78CC80",
        "element.selected": "#4E78CC80",
        "element.disabled": null,

        "ghost_element.background": "#282C3400",
        "ghost_element.hover": "#4E78CC80",
        "ghost_element.active": "#4E78CC80",
        "ghost_element.selected": "#4E78CC80",
        "ghost_element.disabled": null,

        "search.match_background": "#214283",

        "border": "#333841",
        "border.variant": "#333840",

        "text": "#D3D7CF",
        "text.muted": "#D3D7CF",
        "text.accent": "#74ade8ff",
        "text.placeholder": "#808080",
        "text.disabled": null,
        "icon": "#CED0D5",
        "icon.muted": "#ABB2BF",
        "icon.accent": "#4E78CC80",
        "icon.disabled": null,
        "icon.placeholder": null,

        "players": [
          {
            "selection": "#214283BB",
            "background": "#214283FF",
            "cursor": "#BBBBBBFF"
          }
        ],
        "terminal.background": "#21252C",
        "terminal.foreground": null,

        "warning.background": "#21252B",
        "warning.border": "#323844",
        "warning": "#FFE62A3D",
        "hint": "#787878",
        "hint.background": "#2C323C",

        "syntax": {
          "attribute": {
            "color": "#8ae234",
            "font_style": null,
            "font_weight": null
          },
          "boolean": {
            "color": "#AD7FA8"
          },
          "constructor": {
            "color": "#edd400"
          },
          "comment": {
            "color": "#808080",
            "font_style": "italic"
          },
          "constant": {
            "color": "#A9B7C6"
          },
          "function": {
            "color": "#FFC66D"
          },
          "keyword": {
            "color": "#ad7fa8"
          },
          "label": {
            "color": "#edd400",
            "font_weight": null
          },
          "property": {
            "color": "#D3D7CF",
            "font_style": null,
            "font_weight": null
          },
          "primary": {
            "color": "#edd400",
            "font_style": null,
            "font_weight": null
          },
          "string": {
            "color": "#edd400"
          },
          "type": {
            "color": "#D3D7CF"
          },
          "variable": {
            "color": "#A9B7C6"
          },
          "variable.special": {
            "color": "#A9B7C6",
            "font_style": null,
            "font_weight": null
          },
          "variant": {
            "color": "#edd400",
            "font_style": null,
            "font_weight": null
          },
          "punctuation.bracket": {
            "color": "#E8BA36",
            "font_style": null,
            "font_weight": null
          },
          "tag": {
            "color": "#edd400",
            "font_style": null,
            "font_weight": null
          },
          "emphasis": {
            "color": "#edd400",
            "font_style": null,
            "font_weight": null
          },
          "embedded": {
            "color": "#edd400",
            "font_style": null,
            "font_weight": null
          },
          "enum": {
            "color": "#8AE234",
            "font_style": null,
            "font_weight": null
          },
          "number": {
            "color": "#EDD400",
            "font_style": null,
            "font_weight": null
          },
          "operator": {
            "color": "#D3D7CF",
            "font_style": null,
            "font_weight": null
          }
        },
        "terminal": {
          "ansi": {
            "black": "#2e3436",
            "red": "#cc0000",
            "green": "#4e9a06",
            "yellow": "#c4a000",
            "blue": "#3465a4",
            "magenta": "#75507b",
            "cyan": "#06989a",
            "white": "#d3d7cf",
            "bright_black": "#555753",
            "bright_red": "#ef2929",
            "bright_green": "#8ae234",
            "bright_yellow": "#fce94f",
            "bright_blue": "#729fcf",
            "bright_magenta": "#ad7fa8",
            "bright_cyan": "#34e2e2",
            "bright_white": "#eeeeec"
          }
        }
      }
    }
  ]
}
